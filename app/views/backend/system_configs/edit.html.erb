<%= form_for @system_config, :url => backend_system_config_path(@system_config) do |f| %>
  <div class="well">
    <div id="myTabContent" class="tab-content">
      <div class="tab-pane active in" id="home">
        <label><%= (@system_config.key) %></label>
        <%= f.text_field :value, :class => "input-xlarge" %>
      </div>
    </div>
  </div>

  <div class="btn-toolbar form-actions">
    <input type='submit' class="btn btn-primary" value='保存'>
    <div class="btn-group">
    </div>
  </div>

<% end %>
