<%= btn_toolbar_adds([name: '添加统计代码', path: new_backend_web_statistic_path, type: 'add']) %>
<div class="well">
    <table class="table table-hover">
      <thead>
        <tr>
          <th>编号</th>
          <th>栏目名称</th>
          <th>创建时间</th>
          <th style="width: 26px;"></th>
        </tr>
      </thead>
      <tbody>
        <% @web_statistics.each do |p| %>
          <tr>
            <td><%= p.id %></td>
            <td><%= p.title %></td>
            <td><%= p.created_at.strftime('%Y-%M-%d') %></td>
            <td>
              <%= link_to_edit edit_backend_web_statistic_path(p) %>
              <%= link_to_delete backend_web_statistic_path(p)%>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
