<div class="row" id="content-wrapper">
  <div class="col-xs-12">
    <div class="row">
      <%= form_note_account flash %>
      <div class="col-sm-12">
        <div class="page-header">
          <h1 class="pull-left">
            <i class="icon-edit"></i>
            <span>修改自动投标规则</span>
          </h1>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-sm-12">
        <div class="box">
          <div class="box-content">
            <%= form_for @auto_invest_rule, :url => account_auto_invest_path(@auto_invest_rule), :html=>{:class => 'form form-horizontal validate-form', :novalidate=>"novalidate"} do |f| %>
              <%= render :partial => "form", :locals => {:f => f} %>
            <% end%>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>